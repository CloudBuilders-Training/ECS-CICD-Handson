version: 0.0
Resources:
  - TargetService:
      Type: AWS::ECS::Service
      Properties:
        TaskDefinition: "arn:aws:ecs:ap-northeast-1:PLACEHOLDER_AWS_ID:task-definition/PLACEHOLDER_TASK_DEFINITION"
        LoadBalancerInfo:
          ContainerName: "PLACEHOLDER_CONTAINER_NAME"
          ContainerPort: 80